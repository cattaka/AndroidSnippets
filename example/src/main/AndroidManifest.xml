<?xml version="1.0" encoding="utf-8"?>
<manifest package="net.cattaka.android.snippets.example"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".core.MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CodeLabelExampleActivity"/>
        <activity android:name=".SimpleStringExampleActivity"/>
        <activity android:name=".RecyclerViewHeaderExampleActivity"/>
        <activity android:name=".MultiAdapterExampleActivity"/>
        <activity android:name=".ComplexStringExampleActivity"/>
        <activity android:name=".ScrambleAdapterExampleActivity"/>
        <activity android:name=".OperatableListExampleActivity"/>
        <activity android:name=".NestedScrambleAdapterExampleActivity"/>
        <activity android:name=".TreeItemAdapterExampleActivity"/>
        <activity android:name=".ChoosableTreeItemAdapterExampleActivity"/>
        <activity android:name=".GitHubReposActivity"/>
        <activity android:name=".ViewSnapperExampleActivity"/>
    </application>

</manifest>